[dev]
dev.example.com     ansible_host=192.168.64.3

[staging]
staging.example.com ansible_host=192.168.64.31 

[prod]
prod.example.com    ansible_host=192.168.64.32

[dev:vars]
ansible_port=22
ansible_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_connection=ssh

[staging:vars]
ansible_port=22
ansible_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_connection=ssh

[prod:vars]
ansible_port=22
ansible_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_connection=ssh
